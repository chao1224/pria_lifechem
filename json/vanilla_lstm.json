{
  "lstm":{
    "padding_length": 91,
    "different_alphabets_num": 35,
    "embedding_size": 30,
    "first_hidden_size": 100,
    "second_hidden_size": 10
  },
  "compile": {
    "loss": "binary_crossentropy",
    "optimizer": {
      "option":"adam",
      "sgd": {
        "lr":0.3,
        "momentum": 0.9,
        "decay": 0.9,
        "nestrov": true
      },
      "adam": {
        "lr": 0.0001,
        "beta_1": 0.9,
        "beta_2": 0.999,
        "epsilon": 1e-8
      },
      "rmsprop": {
        "lr": 0.0001
      }
    }
  },
  "fitting": {
    "nb_epoch": 10,
    "batch_size": 2048,
    "verbose": 1,
    "early_stopping": {
      "option": "auc",
      "patience": 50
    }
  },
  "batch": {
    "is_use":true,
    "epsilon":1e-6,
    "mode": 0,
    "axis":-1,
    "momentum":0.9,
    "weights": null,
    "beta_init":"zero",
    "gamma_init":"one"
  },
  "enrichment_factor": {
    "ratio_list": [0.02, 0.01, 0.0015, 0.001]
  },
  "class_weight_option": "no_weight"
}
